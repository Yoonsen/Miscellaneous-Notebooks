;;
;;  N SG strong conjugation to a C SG strong synsem
;;

mal-n_sg_strong-for-c_sg_strong-finstr_infl_rule := 
%suffix (t t)(e ett)(a a)(o o)(ø øtt)(å ått)(u u)(i idt)(i itt)(æ ætt)(y ytt)(d dt)(dd dd)(ad ad)(id id)(øyd øyd)(øy øyt)(g gt)(gg gt)(k kt)(kk kt)(l lt)(ll lt)(n nt)(nn nt)(p pt)(pp pt)(m mt)(r rt)(rr rt)(s st)(gs gs)(v vt)(øt øtt)(et ett)(at att)(gt gt)(dsk dsk)(nsk nsk)(gsk gsk)(lsk lsk)(norsk norsk)(kt kt)
orth-alter-masc-or-fem-sg-adj-word &
[GENRE robust & [MALFLECT mal-flect],
 SYNSEM.LOCAL.CAT.HEAD.--GENDER-DISTINCTION + ].

mal-n_sg_strong-for-c_sg_strong-nonfstr_infl_rule := 
%suffix (t t)(es es)(ig ig)(d dt)(g gt)(k kt)(l lt)(n nt)(p pt)(r rt)(s st)(v vt)(sk sk)(e et)(nde nde)(rne rne)(a at)(o ot)(ø øt)(å åt)(u ut)(i it)(æ æt)(y yt)(ille ille)
orth-alter-masc-or-fem-sg-adj-word &
[GENRE robust & [MALFLECT mal-flect],
 SYNSEM.LOCAL.CAT.HEAD.--GENDER-DISTINCTION + ].

;;
;; C SG strong conjugation for N SG strong synsem
;; 

mal-c_sg_strong-for-n_sg_strong-infl_rule :=
neutsg-infl-adj-word-const &
[GENRE robust & [MALFLECT mal-flect],
 SYNSEM.LOCAL.CAT.HEAD.--GENDER-DISTINCTION + ].


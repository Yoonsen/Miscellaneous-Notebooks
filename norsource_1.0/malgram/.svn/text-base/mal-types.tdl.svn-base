[ SYNSEM.LOCAL.CAT.HEAD.MAL.MALINV mal-invy,
  FRONT-TO-FILL -,
  NON-HEAD-DTR.SYNSEM.LOCAL.CTXT non-pron-discbnd,
  GENRE robust & [ MALDROP no-drop ] ].

;; Opening for an adjective-modified N to not be modified by a DET
